{
  "name": "@sammons/typed-mind-renderer",
  "version": "0.1.0",
  "description": "Interactive HTML/D3.js renderer for TypedMind DSL - generates beautiful visualizations of software architectures",
  "keywords": ["typedmind", "dsl", "renderer", "visualization", "d3", "html", "architecture-diagram"],
  "author": "sammons",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sammons/typed-mind-lang"
  },
  "homepage": "https://github.com/sammons/typed-mind-lang#readme",
  "bugs": {
    "url": "https://github.com/sammons/typed-mind-lang/issues"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "build": "tsc --build && npm run copy-static",
    "copy-static": "mkdir -p dist && cp -r src/static dist/",
    "dev": "tsc --build --watch",
    "clean": "rm -rf dist *.tsbuildinfo",
    "test": "vitest --run",
    "test:watch": "vitest --watch",
    "lint": "eslint src",
    "prepublishOnly": "npm run clean && npm run build && npm test"
  },
  "dependencies": {
    "@sammons/typed-mind": "^0.1.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "tsup": "^8.3.5",
    "vitest": "^2.1.8"
  },
  "publishConfig": {
    "access": "public"
  }
}