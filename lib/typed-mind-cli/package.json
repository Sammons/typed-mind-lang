{
  "name": "@sammons/typed-mind-cli",
  "version": "0.1.4",
  "description": "Command-line interface for TypedMind DSL - a declarative language for building software architectures",
  "keywords": [
    "typedmind",
    "dsl",
    "cli",
    "architecture",
    "declarative",
    "software-design"
  ],
  "author": "sammons",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sammons2/typed-mind-lang"
  },
  "homepage": "https://github.com/sammons2/typed-mind-lang#readme",
  "bugs": {
    "url": "https://github.com/sammons2/typed-mind-lang/issues"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "typed-mind": "./dist/cli.js",
    "tmd": "./dist/cli.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "build": "tsc --build && chmod +x dist/cli.js",
    "dev": "tsc --build --watch",
    "clean": "rm -rf dist *.tsbuildinfo",
    "test": "vitest --run --passWithNoTests",
    "test:watch": "vitest --watch",
    "lint": "eslint src",
    "prepublishOnly": "npm run clean && npm run build && npm test"
  },
  "dependencies": {
    "@sammons/typed-mind": "workspace:*",
    "@sammons/typed-mind-renderer": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^22.18.1",
    "tsup": "^8.3.5",
    "vitest": "^3.2.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
